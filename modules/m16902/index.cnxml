        </m:math>
      </equation>
      <para id="id2259992"><link target-id="uid28"/> can be written</para>
      <equation id="uid29">
        <m:math mode="display">
          <m:mrow>
            <m:mi>B</m:mi>
            <m:mo>(</m:mo>
            <m:mi>z</m:mi>
            <m:mo>)</m:mo>
