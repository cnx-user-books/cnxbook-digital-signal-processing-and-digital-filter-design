                      <m:mn>2</m:mn>
                    </m:msup>
                  </m:mrow>
                </m:mfrac>
              </m:msqrt>
              <m:mo>=</m:mo>
              <m:msqrt>
                <m:mrow>
                  <m:msup>
                    <m:mn>10</m:mn>
